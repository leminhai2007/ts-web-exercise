{
    "name": "ts-web-exercise",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "check:all:format": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "check:all:convention": "eslint .",
        "fix:all:format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
        "fix:all:convention": "eslint . --fix",
        "check:staged": "lint-staged",
        "fix:staged": "lint-staged --config .lintstagedrc.fix.json",
        "clean": "rimraf dist node_modules/.vite",
        "preview": "vite preview",
        "serve": "serve dist"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.6",
        "@mui/material": "^7.3.6",
        "react": "^19.2.0",
        "react-dom": "^19.2.0",
        "react-router-dom": "^7.10.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/node": "^24.10.1",
        "@types/react": "^19.2.5",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.1",
        "eslint": "^9.39.1",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-refresh": "^0.4.24",
        "globals": "^16.5.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.2.7",
        "prettier": "^3.7.4",
        "rimraf": "^6.1.2",
        "serve": "^14.2.5",
        "typescript": "~5.9.3",
        "typescript-eslint": "^8.46.4",
        "vite": "^7.2.4",
        "vite-plugin-pwa": "^1.2.0",
        "workbox-window": "^7.4.0"
    }
}
